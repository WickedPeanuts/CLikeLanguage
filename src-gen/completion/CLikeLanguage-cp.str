  is-left-recursive :
    "Primitive" -> <is-left-recursive> "Factor"
    where <not(is-lexical-completion-sort)> "Primitive"

  is-right-recursive :
    "Primitive" -> <is-right-recursive> "Factor"
    where <not(is-lexical-completion-sort)> "Primitive"

  is-Factor-completion-sort :
