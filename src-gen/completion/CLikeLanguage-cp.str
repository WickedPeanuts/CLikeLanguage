rules

  is-left-recursive :
    "Parameter" -> <id>

  suggest-completions-left-rec(|completions):
    (sort, orig-term) -> <add-completions(
                          | ( "Separator"
                            , Separator(
                                orig-term
                              , <try(inline-completions(|Parameter-Plhdr()))> Parameter-Plhdr()
                              )
                            )
                          )
                          ; fail> completions
    where <is-Parameter-completion-sort> sort

  is-left-recursive :
    "Expression" -> <id>

  suggest-completions-left-rec(|completions):
    (sort, orig-term) -> <add-completions(
                          | ( "Term"
                            , Term(
                                orig-term
                              , <try(inline-completions(|Expression-Plhdr()))> OperatorTerm-Plhdr()
                              , <try(inline-completions(|Expression-Plhdr()))> Term-Plhdr()
                              )
                            )
                          )
                          ; fail> completions
    where <is-Expression-completion-sort> sort

  is-Expression-completion-sort :
    "Expression" -> <id>

  is-left-recursive :
    "Term" -> <id>

  suggest-completions-left-rec(|completions):
    (sort, orig-term) -> <add-completions(
                          | ( "Factor"
                            , Factor(
                                orig-term
                              , <try(inline-completions(|Term-Plhdr()))> OperatorFactor-Plhdr()
                              , <try(inline-completions(|Term-Plhdr()))> Factor-Plhdr()
                              )
                            )
                          )
                          ; fail> completions
    where <is-Term-completion-sort> sort

  is-Term-completion-sort :
    "Term" -> <id>

  is-right-recursive :
    "Parameter" -> <id>

  suggest-completions-right-rec(|completions):
    (sort, orig-term) -> <add-completions(
                          | ( "Separator"
                            , Separator(
                                <try(inline-completions(|Parameter-Plhdr()))> Parameter-Plhdr()
                              , orig-term
                              )
                            )
                          )
                          ; fail> completions
    where <is-Parameter-completion-sort> sort

  is-Parameter-completion-sort :
    "Parameter" -> <id>

  is-left-recursive :
    "Structure" -> <is-left-recursive> "Program"
    where <not(is-lexical-completion-sort)> "Structure"

  is-right-recursive :
    "Structure" -> <is-right-recursive> "Program"
    where <not(is-lexical-completion-sort)> "Structure"

  is-Program-completion-sort :
    "Structure" -> <id>

  is-left-recursive :
    "Function" -> <is-left-recursive> "Program"
    where <not(is-lexical-completion-sort)> "Function"

  is-right-recursive :
    "Function" -> <is-right-recursive> "Program"
    where <not(is-lexical-completion-sort)> "Function"

  is-Program-completion-sort :
    "Function" -> <id>

  is-left-recursive :
    "If" -> <is-left-recursive> "Statement"
    where <not(is-lexical-completion-sort)> "If"

  is-right-recursive :
    "If" -> <is-right-recursive> "Statement"
    where <not(is-lexical-completion-sort)> "If"

  is-Statement-completion-sort :
    "If" -> <id>

  is-left-recursive :
    "Variable" -> <is-left-recursive> "Statement"
    where <not(is-lexical-completion-sort)> "Variable"

  is-right-recursive :
    "Variable" -> <is-right-recursive> "Statement"
    where <not(is-lexical-completion-sort)> "Variable"

  is-Statement-completion-sort :
    "Variable" -> <id>

  is-left-recursive :
    "Expression" -> <is-left-recursive> "Argument"
    where <not(is-lexical-completion-sort)> "Expression"

  is-right-recursive :
    "Expression" -> <is-right-recursive> "Argument"
    where <not(is-lexical-completion-sort)> "Expression"

  is-Argument-completion-sort :
    "Expression" -> <id>

  is-left-recursive :
    "ExpressionComparator" -> <is-left-recursive> "Expression"
    where <not(is-lexical-completion-sort)> "ExpressionComparator"

  is-right-recursive :
    "ExpressionComparator" -> <is-right-recursive> "Expression"
    where <not(is-lexical-completion-sort)> "ExpressionComparator"

  is-Expression-completion-sort :
    "ExpressionComparator" -> <id>

  is-left-recursive :
    "Term" -> <is-left-recursive> "Expression"
    where <not(is-lexical-completion-sort)> "Term"

  is-right-recursive :
    "Term" -> <is-right-recursive> "Expression"
    where <not(is-lexical-completion-sort)> "Term"

  is-Expression-completion-sort :
    "Term" -> <id>

  is-left-recursive :
    "Factor" -> <is-left-recursive> "Term"
    where <not(is-lexical-completion-sort)> "Factor"

  is-right-recursive :
    "Factor" -> <is-right-recursive> "Term"
    where <not(is-lexical-completion-sort)> "Factor"

  is-Term-completion-sort :
